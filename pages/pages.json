{
  "routes": {
    "store": {
      "path": "/"
    },
    "store/home": {
      "path": "/"
    },
    "store/account": {
      "path": "/account",
      "login": true
    },
    "store/account/orders": {
      "path": "/account/orders",
      "login": true
    },
    "store/compare": {
      "path": "/compare"
    },
    "store/login": {
      "path": "/login"
    },
    "store/brand": {
      "path": "/:brand/b",
      "context": "ProductSearch"
    },
    "store/department": {
      "path": "/:department/d",
      "context": "ProductSearch"
    },
    "store/category": {
      "path": "/:department/:category/c",
      "context": "ProductSearch"
    },
    "store/subcategory": {
      "path": "/:department/:category/:subcategory/sc",
      "context": "ProductSearch"
    },
    "store/product": {
      "path": "/:slug/p",
      "context": "Product"
    },
    "store/search": {
      "path": "/:term/s",
      "context": "ProductSearch"
    }
  },
  "templates": {
    "store": {
      "component": "StoreTemplate"
    },
    "login": {
      "component": "LoginTemplate",
      "extensions": {
        "container": {
          "component": "vtex.render-runtime/ExtensionContainer"
        }
      }
    },
    "compare": {
      "component": "CompareTemplate"
    },
    "home": {
      "component": "HomeTemplate",
      "extensions": {
        "container": {
          "component": "vtex.render-runtime/ExtensionContainer"
        }
      }
    },
    "account": {
      "component": "AccountTemplate",
      "extensions": {
        "container": {
          "component": "vtex.render-runtime/ExtensionContainer"
        }
      }
    },
    "orders": {
      "component": "OrdersTemplate",
      "extensions": {
        "container": {
          "component": "vtex.render-runtime/ExtensionContainer"
        },
        "container/1": {
          "component": "vtex.my-orders-app/index"
        }
      }
    },
    "productSearch": {
      "component": "ProductSearchTemplate",
      "context": "ProductSearch",
      "extensions": {
        "container": {
          "component": "vtex.render-runtime/ExtensionContainer"
        }
      }
    },
    "product": {
      "component": "ProductTemplate",
      "context": "Product",
      "extensions": {
        "container": {
          "component": "vtex.render-runtime/ExtensionContainer"
        }
      }
    }
  },
  "pages": {
    "store": {
      "default": {
        "template": "store"
      }
    },
    "store/home": {
      "default": {
        "template": "home"
      }
    },
    "store/account": {
      "default": {
        "template": "account"
      }
    },
    "store/account/orders": {
      "default": {
        "template": "orders"
      }
    },
    "store/compare": {
      "default": {
        "template": "compare"
      }
    },
    "store/login": {
      "default": {
        "template": "login"
      }
    },
    "store/brand": {
      "default": {
        "template": "productSearch"
      }
    },
    "store/department": {
      "default": {
        "template": "productSearch"
      }
    },
    "store/category": {
      "default": {
        "template": "productSearch"
      }
    },
    "store/subcategory": {
      "default": {
        "template": "productSearch"
      }
    },
    "store/search": {
      "default": {
        "template": "productSearch"
      }
    },
    "store/product": {
      "default": {
        "template": "product"
      }
    }
  }
}
