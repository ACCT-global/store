{
  "routes": {
    "store": {
      "path": "/",
      "context": "StoreContextProvider"
    },
    "store/home": {
      "path": "/"
    },
    "store/account": {
      "path": "/account",
      "login": true
    },
    "store/account/orders": {
      "path": "/account/orders",
      "login": true
    },
    "store/compare": {
      "path": "/compare"
    },
    "store/login": {
      "path": "/login"
    },
    "store/brand": {
      "path": "/:brand/b",
      "context": "ProductSearchContextProvider"
    },
    "store/department": {
      "path": "/:department/d",
      "context": "ProductSearchContextProvider"
    },
    "store/category": {
      "path": "/:department/:category/c",
      "context": "ProductSearchContextProvider"
    },
    "store/subcategory": {
      "path": "/:department/:category/:subcategory/sc",
      "context": "ProductSearchContextProvider"
    },
    "store/product": {
      "path": "/:slug/p",
      "context": "ProductContextProvider"
    },
    "store/search": {
      "path": "/:term/s",
      "context": "ProductSearchContextProvider"
    }
  }
}
